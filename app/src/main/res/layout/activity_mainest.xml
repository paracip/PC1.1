<?xml version="1.0" encoding="utf-8"?>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:id="@+id/mainest"
        tools:context="com.example.gs.pc10.MainActivity"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:background="#ffffff">
        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar_mainest"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                app:popupTheme="@style/AppTheme.PopupOverlay" />

        </android.support.design.widget.AppBarLayout>
        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/cultural2"
            android:background="@drawable/cultural"
            android:layout_alignParentTop="true"
            android:onClick="culturalon"
            android:layout_alignParentStart="true"
            android:layout_marginTop="70dp"
            android:layout_marginStart="10dp" />

        <ImageButton
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:id="@+id/technical2"
            android:background="@drawable/technical"
            android:layout_alignTop="@+id/cultural2"
            android:layout_marginStart="20dp"
            android:layout_toEndOf="@+id/nss2"
            android:onClick="technicalon" />

        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:background="@drawable/nss"
            android:id="@+id/nss2"
            android:layout_marginTop="35dp"
            android:layout_alignStart="@+id/cultural2"
            android:layout_below="@+id/cultural2"
            android:onClick="nsson" />

        <ImageButton
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:background="@drawable/edboards"
            android:id="@+id/edboard2"
            android:layout_above="@+id/textView5"
            android:layout_alignEnd="@+id/textView3"
            android:onClick="edboardon" />

        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:background="@drawable/eic"
            android:id="@+id/eic2"
            android:layout_alignTop="@+id/scc2"
            android:layout_alignStart="@+id/nss2"
            android:onClick="eicon" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:background="@drawable/scc"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="-5dp"
        android:id="@+id/scc2"
        android:layout_below="@+id/edboard2"
        android:layout_alignStart="@+id/edboard2"
        android:onClick="sccon" />

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Cultural"
            android:gravity="center"
            android:id="@+id/textView2"
            android:layout_below="@+id/cultural2"
            android:layout_alignStart="@+id/cultural2"
            android:layout_alignEnd="@+id/cultural2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Technical"
            android:gravity="center"
            android:id="@+id/textView3"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignEnd="@+id/technical2"
            android:layout_alignStart="@+id/technical2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="NSS"
            android:gravity="center"
            android:id="@+id/textView4"
            android:layout_below="@+id/nss2"
            android:layout_alignStart="@+id/nss2"
            android:layout_alignEnd="@+id/nss2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Editorial Boards"
            android:gravity="center"
            android:layout_marginLeft="8dp"
            android:id="@+id/textView5"
            android:layout_below="@+id/nss2"
            android:layout_alignStart="@+id/textView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="EIC"
            android:gravity="center"
            android:id="@+id/textView6"
            android:layout_alignTop="@+id/textView7"
            android:layout_alignLeft="@+id/eic2"
            android:layout_alignRight="@+id/eic2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="SCC"
            android:gravity="center"
            android:layout_marginLeft="-5dp"
            android:id="@+id/textView7"
            android:layout_below="@+id/scc2"
            android:layout_alignLeft="@+id/scc2"
            android:layout_alignRight="@+id/scc2" />

    </RelativeLayout>